{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "APK Downloader"
   },
   "default_locale": "en",
   "description": "Direct download APK file and install the app manually onto your Android devices.",
   "icons": {
      "128": "images/128.png",
      "16": "images/16.png",
      "48": "images/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "APK Downloader",
   "permissions": [ "tabs", "activeTab", "downloads", "storage", "cookies", "background", "contextMenus", "history", "webRequest", "webRequestBlocking", "https://apk.support/*", "https://*.apk.support/*", "https://www.google.com/", "https://*.google.com/*", "https://*.googleapis.com/*", "http://www.google.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.7"
}
